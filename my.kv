#Framskjermen
<FrontScreen>:
	#namnet som vert brukt seinare til referering
	name: "front"

	#definerar bakgrunnsbiletet
	canvas.before:
		#sjOlve biletet
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'framside/adressabygget.jpg'
        #diffust overlegg over biletet
		Color:
			rgba: 0.976470588,0.976470588,0.976470588,0.7
		Rectangle:
			pos: self.pos
			size: self.size

	#definerer at sida skal vere eit boxlayout med eit bilete fyrst, saa ein knapp	
    BoxLayout:
    	orientation: 'vertical'
		Image:
			source: 'framside/adr_logo.png'
			allow_stretch: False
			keep_ratio: False
		Image:
			source: 'framside/elsys_lite_trans.png'
			allow_stretch: False
			keep_ratio: False
        Button:
            text: 'Til menyen'
            font_size: 70
            color: 0.0705882, 0.4784313725, 0.75294117647, 1
            background_color: [1, 1, 1, 0]
            on_release: root.manager.current = 'main'



#Meny-sida
<MainWidget>:
	name: "main"

    canvas.before:
        Color:
            rgba: 0.976470588, 0.976470588, 0.976470588, 1
        Rectangle:
            pos: self.pos
            size: self.size

	BoxLayout:
		orientation: 'vertical'

		GridLayout:
			size: root.size
			rows: 1

			#overskrifta til sida
			Label:
				text: "MENY"
				font_size: 150
				color: 0.0705882, 0.4784313725, 0.75294117647, 1


		GridLayout:
			size: root.size
			cols: 3

			Button:
				text: 'Klimavalg'
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				font_size: 80
				on_press: root.manager.current = 'klima'
			Button:
				text: "Historisk"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				font_size: 80
				on_press: root.manager.current = 'historisk'
			Button:
				text: "Severdigheter"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				font_size: 80
				on_press: root.manager.current = 'SelectionScreen'
			Button:
				text: "Styr parken"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				font_size: 80
				on_press: root.manager.current = 'parkkontroll'
            Button:
				text: "Veibeskrivelser"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				font_size: 80
				on_press: root.manager.current = 'veibeskrivelser'
			Button:
				text: "Arrangement"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				font_size: 80
				on_press: root.manager.current = 'arrangement'




		GridLayout:
			size: root.size
			rows: 2

			Label:

			Button:
				text: "Hjem"
                color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				font_size: 55
				size_hint: (1, 0.4)
				on_press: root.manager.current = 'front'



#Klimaval-modulen

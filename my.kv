#Framskjermen
<FrontScreen>:
	#namnet som vert brukt seinare til referering
	name: "front"

	#definerar bakgrunnsbiletet
	canvas.before:
		#sjølve biletet
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'test.jpg'
        #diffust overlegg over biletet
		Color:
			rgba: 0.976470588,0.976470588,0.976470588,0.7
		Rectangle:
			pos: self.pos
			size: self.size

	#definerer at sida skal vere eit boxlayout med eit bilete fyrst, så ein knapp	
    BoxLayout:
    	orientation: 'vertical'
		Image:
			source: 'logo.png'
			allow_stretch: False
			keep_ratio: False
        Button:
            text: 'Til menyen'
            font_size: 100
            color: 0.0705882, 0.4784313725, 0.75294117647, 1
            background_color: [1, 1, 1, 0]
            on_release: root.manager.current = 'main'



#Meny-sida
<MainWidget>:
	name: "main"

    canvas.before:
        Color:
            rgba: 0.976470588, 0.976470588, 0.976470588, 1
        Rectangle:
            pos: self.pos
            size: self.size

	BoxLayout:
		orientation: 'vertical'

		GridLayout:
			size: root.size
			rows: 1

			#overskrifta til sida
			Label:
				text: "MENY"
				font_size: 150
				color: 0.0705882, 0.4784313725, 0.75294117647, 1


		GridLayout:
			size: root.size
			cols: 3

			Button:
				text: 'Klimaval'
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'klima'
			Button:
				text: "Historisk"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'historisk'
			Button:
				text: "Severdigheiter"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'SelectionScreen'
			Button:
				text: "Uhm noko anna"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'anna'
            Button:
				text: "Veibeskrivelser"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'veibeskrivelser'
			Button:
				text: "Noko anna 2"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'anna'



		GridLayout:
			size: root.size
			rows: 2

			Label:

			Button:
				text: "Framskjerm"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'front'



#Klimaval-modulen
<KlimaScreen>:
	name: "klima"

    canvas.before:
        Color:
            rgba: 0.976470588, 0.976470588, 0.976470588, 1
        Rectangle:
            pos: self.pos
            size: self.size


	BoxLayout:
		orientation: 'vertical'

		GridLayout:
			size: root.size
			rows: 1

			Label:
				text: "Klimaveljaren"
				font_size: 150
				color: 0.0705882, 0.4784313725, 0.75294117647, 1


		GridLayout:
			orientation: 'vertical'
			size: root.size
			cols: 4
			rows: 2

			Image:
				source: 'klimabilde/syden.jpeg'
				allow_stretch: True
				keep_ratio: False
			Image:
				source: 'klimabilde/antarktis.jpeg'
				allow_stretch: True
				keep_ratio: False
			Image:
				source: 'klimabilde/jungel.jpeg'
				allow_stretch: True
				keep_ratio: False
			Image:
				source: 'klimabilde/hav.jpeg'
				allow_stretch: True
				keep_ratio: False
			Button:
				text: 'Syden'
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				on_press: root.playsS()
			Button:
				text: "Antarktis"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				on_press: root.playsI()
			Button:
				text: "Jungel"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				on_press: root.playsJ()
			Button:
				text: "Hav"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				on_press: root.manager.current = 'severdig'



		GridLayout:
			size: root.size
			rows: 2

			Label:

			Button:
				text: "Framskjerm"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.stopAll()
				on_release: root.manager.current = 'main'
				
<SelectionScreen>:
    padding: [10, 20]
    spacing: 2

    BoxLayout:
        orientation: 'vertical'

        GridLayout:
            size_hint: (1, 0.2)
            rows: 1

            Label:
                text: "Kryss av for aktuelle alternativer"
                font_size: 30

        GridLayout:
            orientation: 'vertical'
            size_hint: (1, 1)

            padding: [10, 40]
            cols: 2
            rows: 6
            Label:
                text: "Museum"
            CheckBox:
                on_active: root.checkbox_museum_cliked(self, self.active)
                size_hint_x: .55
            Label:
                text: "Historiske steder"
            CheckBox:
                on_active: root.checkbox_historisk_cliked(self, self.active)
                size_hint_x: .55
            Label:
                text: "Kultur"
            CheckBox:
                on_active: root.checkbox_kultur_cliked(self, self.active)
                size_hint_x: .55
            Label:
                text: "Natur"
            CheckBox:
                on_active: root.checkbox_natur_cliked(self, self.active)
                size_hint_x: .55
            Label:
                text: "Mat og drikke"
            CheckBox:
                on_active: root.checkbox_mat_cliked(self, self.active)
                size_hint_x: .55
            Label:
                text: "Aktiviteter"
            CheckBox:
                on_active: root.checkbox_aktiviteter_cliked(self, self.active)
                size_hint_x: .55

        GridLayout:
            orientation: 'vertical'
            size_hint: (1, 0.11)
            rows: 1
            cols: 2

            Button:
                text: "Klikk når du er klar"
                on_press: root.screenSelector()
                on_release:
                    root.manager.current = str(root.manager.ids.selection_screen.testScreen)
            Button:
                text: 'Til menyen'
                on_press: root.manager.current = 'main'

#Historie-modul
<HistorieScreen>:
	name: 'historisk'

	canvas.before:
        Color:
            rgba: 0.976470588, 0.976470588, 0.976470588, 1
        Rectangle:
            pos: self.pos
            size: self.size

	BoxLayout:
    	orientation: 'vertical'
    	
        Label:
        	text: 'Historisk'
        	color: 0.0705882, 0.4784313725, 0.75294117647, 1
        	font_size: 100
        	size_hint: (1, 0.2)
        

        #Ein rullbar modul der dei ulike historiske plassane er lista
        ScrollView:

			GridLayout:
				cols: 2
				size_hint_y: None
				height: 1000
        	
				# -----------------------------------------------------
				GridLayout:
					rows: 2
					cols: 1

					Label:
						text: 'Nidarosdomen'
						font_size: 50
						size_hint: (1, 1)
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
					Label: 
						text: 'Den praktfulle Nidarosdomen er Norges opprinnelige erkebiskopkirke, og en av nordens største middelalderkirker. Byggingen ble påbegynt i 1070, og domen har siden stått oppreist over Olav den Helliges grav. Flere av Norges konger er både kronet, viet og gravlagt her. Her kan du delta på både gudstjenester, omvisninger og konserter.'
						text_size: self.width, None
						size_hint_y: None
						height: self.texture_size[1]
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
				GridLayout:
					cols: 1
					rows: 1
					size_hint: (0.2, 0.5)

					Image:
						source: 'test2.jpg'
						allow_stretch: True
						keep_ratio: False
				# -----------------------------------------------------
				GridLayout:
					rows: 2
					cols: 1

					Label:
						text: 'Stiftsgården'
						font_size: 50
						size_hint: (1, 1)
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
					Label: 
						text: 'Stiftsgården er Trondheims kongelige residens, og nordens største trepalé. Gården ble bygget for Cecilie Christine Schøller i 1774-1778. Du kan oppleve den barokk-klassisistiske stilen på Stiftsgården ved å delta på omvisning.'
						text_size: self.width, None
						size_hint_y: None
						height: self.texture_size[1]
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
				GridLayout:
					cols: 1
					rows: 1
					size_hint: (0.2, 0.5)

					Image:
						source: 'test2.jpg'
						allow_stretch: True
						keep_ratio: False
				# -----------------------------------------------------
				GridLayout:
					rows: 2
					cols: 1

					Label:
						text: 'Gamle Bybro'
						font_size: 50
						size_hint: (1, 1)
					Label: 
						text: 'Gamle Bybro, også kalt “Lykkens Portal”, strekker seg over nidelven ved Bakklandet. Den ble første gang bygget i forbindelse med Kristiansten festning i 1681, og skulle erstatte den tidligere bybroen. Dagens versjon av broen er fra 1861, og hadde opprinnelig broklapper som kunne heises opp og holdes oppe av to utskårne portalene, hvor én av dem fortsatt står igjen.'
						text_size: self.width, None
						size_hint_y: None
						height: self.texture_size[1]
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
				GridLayout:
					cols: 1
					rows: 1
					size_hint: (0.2, 0.5)

					Image:
						source: 'test2.jpg'
						allow_stretch: True
						keep_ratio: False

				# -----------------------------------------------------
				GridLayout:
					rows: 2
					cols: 1

					Label:
						text: 'Kristiansten festning'
						font_size: 50
						size_hint: (1, 1)
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
					Label: 
						text: 'Kristiansten festning har siden slutten av 1600-tallet stått over Bakklandet i Trondheim, og voktet over Gamle Bybro. Frem til 2001 har Kristiansten blant annet vært benyttet til militært bruk, treningsanlegg, brannvakt og rettersted. I dag er Kristiansten festning administrativt fredet og et av Norges 14 nasjonale festningsverk.'
						text_size: self.width, None
						size_hint_y: None
						height: self.texture_size[1]
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
				GridLayout:
					cols: 1
					rows: 1
					size_hint: (0.2, 0.5)

					Image:
						source: 'test2.jpg'
						allow_stretch: True
						keep_ratio: False

				# -----------------------------------------------------
				GridLayout:
					rows: 2
					cols: 1

					Label:
						text: 'Erkebispegården'
						font_size: 50
						size_hint: (1, 1)
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
					Label: 
						text: 'Erkebispegården var fra gammelt av erkebiskopens bolig og administrative sentrum i Norge. Her er Norges riksregalier, orginalskulpturer fra Nidarosdomen og arkeologiske funn fra Erkebispegårdens historie utstilt.'
						text_size: self.width, None
						size_hint_y: None
						height: self.texture_size[1]
						color: 0.0705882, 0.4784313725, 0.75294117647, 1
						background_color: [1, 1, 1, 0]
				GridLayout:
					cols: 1
					rows: 1
					size_hint: (0.2, 0.5)

					Image:
						source: 'test2.jpg'
						allow_stretch: True
						keep_ratio: False

        Button:
            text: 'Til menyen'
            color: 0.0705882, 0.4784313725, 0.75294117647, 1
			background_color: [1, 1, 1, 0.1]
            on_press: root.manager.current = 'main'
            size_hint: (1, 0.2)



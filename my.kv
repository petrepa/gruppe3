<FrontScreen>:
	name: "front"


	canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'test.jpg'
    BoxLayout:
    	orientation: 'vertical'
        Label:
            text: 'ADRESSA'
            font_size: 150
            color: 0.03137, 0.32549, 0.46666666666, 1
        Button:
            text: 'Til menyen'
            color: 0.0705882, 0.4784313725, 0.75294117647, 1
            background_color: [1, 1, 1, 0]
            on_release: root.manager.current = 'main'




<MainWidget>:
	name: "main"

    canvas.before:
        Color:
            rgba: 0.976470588, 0.976470588, 0.976470588, 1
        Rectangle:
            pos: self.pos
            size: self.size

	BoxLayout:
		orientation: 'vertical'

		GridLayout:
			size: root.size
			rows: 1

			Label:
				text: "MENY"
				font_size: 150
				color: 0.0705882, 0.4784313725, 0.75294117647, 1


		GridLayout:
			size: root.size
			cols: 3

			Button:
				text: 'Klimaval'
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'klima'
			Button:
				text: "Historisk"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'historisk'
			Button:
				text: "Severdigheiter"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'severdig'
			Button:
				text: "Uhm noko anna"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'anna'
            Button:
				text: "Veibeskrivelser"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'veibeskrivelser'
			Button:
				text: "Noko anna 2"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'anna'



		GridLayout:
			size: root.size
			rows: 2

			Label:

			Button:
				text: "Framskjerm"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0]
				on_press: root.manager.current = 'front'




<KlimaScreen>:
	name: "klima"


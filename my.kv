			Button:
				text: "Jungel"
				font_size: 50
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				on_press: root.jungel(self)
			Button:
				text: "Hav"
				font_size: 50
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				on_press: root.hav(self)
				



		GridLayout:
			size: root.size
			rows: 2

			Label:

			Button:
				text: "Tilbake til menyen"
				color: 0.0705882, 0.4784313725, 0.75294117647, 1
				background_color: [1, 1, 1, 0.1]
				font_size: 70
